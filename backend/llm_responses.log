    {
      "day_number": 30,
      "title": "Course Wrap-up, Future Directions, and Ethical Implications",
      "is_project_day": false,
      "content": {
        "type": "doc",
        "content": [
          {
            "type": "heading",
            "attrs": {
              "level": 2
            },
